[1mSYNOPSIS[0m[m
       [1m#include[0m [1m<unistd.h>[0m[m
[m
       [1mssize_t[0m  [1mwrite(int[0m  [4mfd[24m[1m,[0m [1mconst[0m [1mvoid[0m [1m*[0m[4mbuf[24m[1m,[0m [1msize_t[0m[m
       [4mcount[24m[1m);[0m[m
[m
[1mDESCRIPTION[0m[m
       [1mwrite[0m() writes up to [4mcount[24m bytes from the  buf-[m
       fer  pointed [4mbuf[24m to the file referred to by the[m
       file descriptor [4mfd[24m.[m
