[1mNAME[0m[m
       open,  creat  - open and possibly[m
       create a file or device[m
[m
[1mSYNOPSIS[0m[m
       [1m#include[0m [1m<sys/types.h>[0m[m
       [1m#include[0m [1m<sys/stat.h>[0m[m
       [1m#include[0m [1m<fcntl.h>[0m[m
[m
       [1mint[0m [1mopen(const[0m [1mchar[0m [1m*[0m[4mpathname[24m[1m,[0m [1mint[0m [m [4mflags[24m[1m);[0m[m
       [1mint[0m [1mopen(const[0m [1mchar[0m [1m*[0m[4mpathname[24m[1m,[0m [1mint[0m [m [4mflags[24m[1m,[0m [1mmode_t[0m [4mmode[24m[1m);[0m[m
[m
       [1mint[0m [1mcreat(const[0m [1mchar[0m [1m*[0m[4mpathname[24m[1m,[0m [1mmod[0m[m [1me_t[0m [4mmode[24m[1m);[0m[m
[m
[1mDESCRIPTION[0m[m
       Given  a  [4mpathname[24m  for  a  file,[m
       [1mopen[0m() returns a file descriptor,[m
       a small, nonnegative integer  for[m
       use  in  subsequent  system calls[m
       ([1mread[0m(2),   [1mwrite[0m(2),   [1mlseek[0m(2),[m
       [1mfcntl[0m(2),    etc.).    The   file[m
       descriptor returned by a success‐[m
       ful  call will be the lowest-num‐[m
       bered file  descriptor  not  cur‐[m
[7m) line 1 (press h for help or q to quit)[27m[K[K [KESCESC[KOO[KBB[K       rently open for the process.[m
[7m) line 2 (press h for help or q to quit)[27m[K[K [KESCESC[KOO[KBB[K[m
[7m) line 5 (press h for help or q to quit)[27m[K[K [KESCESC[KOO[KBB[K       By default, the new file descrip‐[m
[7m) line 6 (press h for help or q to quit)[27m[K[K [KESCESC[KOO[KBB[K       tor is set to remain open  across[m
[7m) line 7 (press h for help or q to quit)[27m[K[K [KESCESC[KOO[KBB[K       an     [1mexecve[0m(2)    (i.e.,    the[m
[7m) line 8 (press h for help or q to quit)[27m[K[K [KESCESC[KOO[KBB[K       [1mFD_CLOEXEC[0m file  descriptor  flag[m
[7m) line 9 (press h for help or q to quit)[27m[K[K [KESCESC[KOO[KBB[K       described  in  [1mfcntl[0m(2)  is  ini‐[m
[7m line 10 (press h for help or q to quit)[27m[K[K [KESCESC[KOO[KBB[K       tially  disabled;  the  [1mO_CLOEXEC[0m[m
[7m line 11 (press h for help or q to quit)[27m[K[K [KESCESC[KOO[KBB[K       flag,  described  below,  can  be[m
